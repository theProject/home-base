                {
                  "mode": "normal",
                  "text": "Step 1",
                  "type": "text",
                  "style": "",
                  "detail": 0,
                  "format": 1,
                  "version": 1
                },
                {
                  "mode": "normal",
                  "text": ": Declutter your desk; keep only essentials.",
                  "type": "text",
                  "style": "",
                  "detail": 0,
                  "format": 0,
                  "version": 1
                }
              ],
              "direction": "ltr",
              "textStyle": "",
